GET     /                           controllers.MADController.index

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /information                controllers.MADController.information
GET     /questions                  controllers.MADController.questions
GET     /tree                       controllers.MADController.treeNoPath()
GET     /tree/*path                 controllers.MADController.tree(path)
GET     /answer/*path               controllers.MADController.answer(path)
GET     /newconceptoid              controllers.MADController.newconceptoid
